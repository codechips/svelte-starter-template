{
  "name": "svelte-typescript-rollup-boilerplate",
  "version": "1.1.0",
  "scripts": {
    "start": "rimraf dist && run-p watch:*",
    "clean": "rimraf dist",
    "check": "svelte-check",
    "build": "run-s clean check compile",
    "format": "prettier --write src",
    "compile": "cross-env NODE_ENV=production rollup --config",
    "watch:check": "svelte-check --watch",
    "watch:build": "cross-env NODE_ENV=development rollup --config --watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@swc/core": "^1.2.19",
    "@tsconfig/svelte": "^1.0.8",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.1",
    "rollup-plugin-html2": "^2.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.24.1",
    "svelte-check": "^1.0.14",
    "svelte-preprocess": "^4.0.11",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "sirv-cli": "^1.0.6"
  }
}
